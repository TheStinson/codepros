<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>CodePros | Home</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

  <!-- Theme style -->
        <%= stylesheet_link_tag "_all-skins.min" %>
        <%= stylesheet_link_tag "AdminLTE.min" %>
  <!-- Ionicons -->
        <%= stylesheet_link_tag "https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" %>
  <!-- Bootstrap 3.3.5 -->
         <%= stylesheet_link_tag "bootstrap.min" %>
  <!-- Font Awesome -->
          <%= stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" %>
  
  <!-- jvectormap -->
          <%= stylesheet_link_tag "jquery-jvectormap-1.2.2" %>
          
  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <%= javascript_include_tag "https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js" %>
  <%= javascript_include_tag "https://oss.maxcdn.com/respond/1.4.2/respond.min.js"%>
  <![endif]-->
</head>

<body class="hold-transition skin-green layout-top-nav">
<div class="wrapper">

  <header class="main-header">
    <nav class="navbar navbar-static-top">
      <div class="container">
        <div class="navbar-header">
          <a href="#" class="navbar-brand"><b><%= image_tag("/img/logo.png", :alt => "CODE PROS") %></b></a>
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
            <i class="fa fa-bars"></i>
          </button>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="dropdown">
            <li><a href="contest.aspx"><i class="fa fa-user" ></i> <span class="h4">Developers</span></a></li>
            <li><a href="#"><i class="fa fa-briefcase"></i> <span class="h4">Companies</span></a></li>
            <li><a href="#"><i class="fa fa-graduation-cap"></i> <span class="h4">Institutes</span></a></li>
          </ul>
        </div>
        <!-- /.navbar-collapse -->
        <!-- Navbar Right Menu -->
      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">

            <li>
              <a href="#">
                <span class="hidden-xs">Welcome, Guest!</span>
            </a>
            </li>

            <li>
            <a href="signup.aspx">
           
                <i class="fa fa-edit"></i>
                <span class="hidden-xs">Sign Up</span>
            </a>
            </li>

            <li>
            <a href="login.aspx">
              
                <i class="fa fa-sign-in"></i>
                <span class="hidden-xs">Login</span>
            </a>
            </li>
        </ul>
      </div>
        <!-- /.navbar-custom-menu -->
      </div>
      <!-- /.container-fluid -->
    </nav>
  </header>
  <!-- Full Width Column -->
  <div class="content-wrapper">
    <div class="container">
<%= yield %>

</div>
    <!-- /.container -->
  </div>
  <!-- /.content-wrapper -->
  <footer class="main-footer">
    <div class="container">
      <div class="pull-right hidden-xs">
        <b>Version</b> 1.0.0
      </div>
      <strong>Copyright &copy; 2015-2016 <a href="http://cbainc.in">CBA INC</a>.</strong> All rights
      reserved.
    </div>
    <!-- /.container -->
  </footer>
</div>
<!-- ./wrapper -->

<!-- jQuery 2.1.4 -->

<%= javascript_include_tag "jQuery-2.1.4.min.js" %>
<!-- Bootstrap 3.3.5 -->
<%= javascript_include_tag "bootstrap.min.js" %>
<!-- FastClick -->
<%= javascript_include_tag "fastclick.js" %>
<!-- AdminLTE App -->
<%= javascript_include_tag "app.min.js" %>
<!-- Sparkline -->
<%= javascript_include_tag "jquery.sparkline.min.js" %>
<!-- jvectormap -->
<%= javascript_include_tag "jquery-jvectormap-1.2.2.min.js" %>
<%= javascript_include_tag "jquery-jvectormap-world-mill-en.js" %>
<!-- SlimScroll 1.3.0 -->
<%= javascript_include_tag "jquery.slimscroll.min.js" %>
<!-- ChartJS 1.0.1 -->
<%= javascript_include_tag "Chart.min.js" %>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<%= javascript_include_tag "dashboard2.js" %>
<!-- AdminLTE for demo purposes -->
<%= javascript_include_tag "demo.js" %>
</body>
</html>

